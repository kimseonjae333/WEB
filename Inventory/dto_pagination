package kr.ac.kopo.ctc.kopo39.dto;

public class Pagination { //7개 변수 지정
	private int C; //현재페이지(=현재블록)
	private int S; //스타트블록
	private int E; //앤드블록
	private int P; //현재페이지(블록)-블록 사이즈
	private int PP; //맨 앞 블록
	private int N; //현재페이지(블록)+블록 사이즈
	private int NN; //맨 뒤 블록
	
	//getter,setter 만들기
	public int getC() {
		return C;
	}
	public void setC(int c) {
		C = c;
	}
	public int getS() {
		return S;
	}
	public void setS(int s) {
		S = s;
	}
	public int getE() {
		return E;
	}
	public void setE(int e) {
		E = e;
	}
	public int getP() {
		return P;
	}
	public void setP(int p) {
		P = p;
	}
	public int getPP() {
		return PP;
	}
	public void setPP(int pP) {
		PP = pP;
	}
	public int getN() {
		return N;
	}
	public void setN(int n) {
		N = n;
	}
	public int getNN() {
		return NN;
	}
	public void setNN(int nN) {
		NN = nN;
	}
	
	
}
	
